{
  "id": "http://some.site.somewhere/entry-schema#",
  "$schema": "http://json-schema.org/draft-054/schema#",
  "description": "Schema for Jira project extractor",
  "type": "object",
  "properties": {
    "projects": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "key": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "projectTypeKey": {
            "type": "string"
          },
          "self": {
            "type": "string"
          },
          "projectCategory": {
            "type": "object",
            "properties": {
              "name" : {
                "type": "string"
              }
            }
          }
        }
      }
    }
  }
}